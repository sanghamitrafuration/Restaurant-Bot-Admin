{
  "name": "restaurant-bot-admin",
  "version": "1.0.0",
  "main": "dist/main/server.js",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig-build.json",
    "build:watch": "tsc -p tsconfig-build.json --watch",
    "start": "node dist/main/server.js",
    "dev": "yarn run build && nodemon -L --watch ./dist ./dist/main/server.js",
    "start:dev": "concurrently --kill-others-on-fail \"yarn run build:watch\" \"yarn run dev\"",
    "test": "jest --passWithNoTests --runInBand --no-cache",
    "test:staged": "yarn run test -- --findRelatedTests",
    "test:ci": "yarn run test -- --coverage",
    "lint": "eslint --ignore-path .gitignore --ext .ts --fix prettier --check .",
    "prepare": "husky install"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/plugin-transform-runtime": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-typescript": "^7.22.5",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@shelf/jest-mongodb": "^4.1.7",
    "@types/babel__traverse": "^7.20.1",
    "@types/jest": "^29.5.3",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^20.4.4",
    "@types/supertest": "^2.0.12",
    "concurrently": "^8.2.0",
    "eslint-config-standard-with-typescript": "^37.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^8.0.0",
    "jest": "^29.6.1",
    "lint-staged": "^13.2.3",
    "prettier": "^3.0.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.398.0",
    "@aws-sdk/client-sns": "^3.398.0",
    "@aws-sdk/s3-request-presigner": "^3.398.0",
    "@types/cassandra-driver": "^4.2.0",
    "@types/express": "^4.17.17",
    "aws-sdk": "^2.1425.0",
    "cassandra-driver": "^4.6.4",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-cassandra": "^2.9.0",
    "joi": "^17.10.1",
    "module-alias": "^2.2.3",
    "monet": "^0.9.3",
    "mongodb": "^5.7.0",
    "mongoose": "^7.5.0",
    "node": "^20.5.1",
    "nodemon": "^3.0.1"
  },
  "_moduleAliases": {
    "@domain": "dist/domain",
    "@data": "dist/data",
    "@presentation": "dist/presentation",
    "@main": "dist/main"
  }
}
